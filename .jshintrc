{
  "maxerr"        : 50,

  // Enforcing
  "bitwise"       : true,
  "camelcase"     : false,
  "curly"         : true,
  "eqeqeq"        : true,
  "forin"         : true,
  "freeze"        : true,
  "immed"         : false,
  "latedef"       : false,
  "newcap"        : false,
  "noarg"         : true,
  "noempty"       : true,
  "nonbsp"        : true,
  "nonew"         : false,
  "plusplus"      : false,
  "quotmark"      : "double",
  //   false    : do nothing
  //   true     : ensure what
  //   "single" : require sin
  //   "double" : require dou
  "undef"         : true,
  "unused"        : true,
  //   true     : all variabl
  //   "vars"   : all variabl
  //   "strict" : all variabl
  "strict"        : false,
  "maxparams"     : false,
  "maxdepth"      : false,
  "maxstatements" : false,
  "maxcomplexity" : false,
  "maxlen"        : false,
  "varstmt"       : false,

  // Relaxing
  "asi"           : false,
  "boss"          : false,
  "debug"         : false,
  "eqnull"        : false,
  "esversion"     : 6,
  "moz"           : false,
  // (ex: `for each`, multipl
  "evil"          : false,
  "expr"          : false,
  "funcscope"     : false,
  "globalstrict"  : false,
  "iterator"      : false,
  "lastsemic"     : false,
  "laxbreak"      : false,
  "laxcomma"      : false,
  "loopfunc"      : false,
  "multistr"      : false,
  "noyield"       : false,
  "notypeof"      : false,
  "proto"         : false,
  "scripturl"     : false,
  "shadow"        : false,
  "sub"           : false,
  "supernew"      : false,
  "validthis"     : false,

  // Environments
  "browser"       : false,
  "browserify"    : false,
  "couch"         : false,
  "devel"         : false,
  "dojo"          : false,
  "jasmine"       : false,
  "jquery"        : false,
  "mocha"         : false,
  "mootools"      : false,
  "node"          : true,
  "nonstandard"   : false,
  "phantom"       : false,
  "prototypejs"   : false,
  "qunit"         : false,
  "rhino"         : false,
  "shelljs"       : false,
  "typed"         : false,
  "worker"        : false,
  "wsh"           : false,
  "yui"           : false,

  // Custom Globals
  "globals"       : {}
}
